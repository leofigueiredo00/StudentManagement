@{
}
@model IEnumerable<StudentManagement.Models.Student>
<h1>Students List</h1>
<h2>Leonardo Figueiredo</h2>
<h2>200577759</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Course</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.name</td>
                <td>@student.age</td>
                <td>@student.course</td>
            </tr>
        }
    </tbody>
</table>
